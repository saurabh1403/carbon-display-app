<SampleXml>
	<UI-to-Native>
		<message>
			<callIdentifier>{34565344-56-576-657-67-6AD}</callIdentifier>
			<messagetype>async</messagetype>
			<sessionbased>false</sessionbased>
			<sessionid/>
			<targetobject>closeIDAppSession</targetobject>
			<argumentXml>
				<!--This pkg id is the pkg on which user clicked and it will be treated as the session id-->
				<packageId>{111}</packageId>
			</argumentXml>
		</message>
	</UI-to-Native>
	<!--in case of Error-->
	<Native-toUI>
		<error>some error is there</error>
	</Native-toUI>
	<!--in case of successful opening of session-->
	<Native-toUI>
		<result>NativeProcessClosed</result>
	</Native-toUI>
</SampleXml>
